Q1. Print Welcome
#include <stdio.h>

int main() {
    printf("Welcome");
    return 0;
}

Q2. Print the given word
#include <stdio.h>

int main() {
    printf("Hello");
    return 0;
}



Q3. Print the given Message
#include <stdio.h>

int main() {
    printf("Good Morning Everyone!");
    return 0;
}


Q4. Print the given integer number
#include <stdio.h>

int main() {
    int num = 25;    // You can change 25 to any number
    printf("%d", num);
    return 0;
}

Q5. Print the given fractional (float) number
#include <stdio.h>

int main() {
    float num = 12.345;
    printf("%f", num);
    return 0;
}

Q6. Print fractional number in 2-digit decimal format
#include <stdio.h>

int main() {
    float num = 12.345;
    printf("%.2f", num);   // prints only 2 digits after decimal
    return 0;
}

Day-2
Q7. Print the ASCII value of a character
#include <stdio.h>

int main() {
    char ch = 'A';
    printf("ASCII value of %c = %d", ch, ch);
    return 0;
}

Q8. Print the character for a given ASCII value
#include <stdio.h>

int main() {
    int ascii = 66;   // ASCII value for 'B'
    printf("Character for ASCII %d = %c", ascii, ascii);
    return 0;
}

Q9. Print two numbers with a tab space between them
#include <stdio.h>

int main() {
    int a = 10, b = 20;
    printf("%d\t%d", a, b);   // \t creates a tab space
    return 0;
}

Q10. Print two numbers in two lines
#include <stdio.h>

int main() {
    int a = 10, b = 20;
    printf("%d\n%d", a, b);   // \n moves to next line
    return 0;
}

Q11. Print the size of char, int, float, and double
#include <stdio.h>

int main() {
    printf("Size of char: %lu bytes\n", sizeof(char));
    printf("Size of int: %lu bytes\n", sizeof(int));
    printf("Size of float: %lu bytes\n", sizeof(float));
    printf("Size of double: %lu bytes\n", sizeof(double));
    return 0;
}

Q12. Print your marks in 5 subjects each on a new line
#include <stdio.h>

int main() {
    int m1 = 85, m2 = 90, m3 = 78, m4 = 88, m5 = 92;
    printf("Marks in 5 subjects:\n");
    printf("%d\n%d\n%d\n%d\n%d", m1, m2, m3, m4, m5);
    return 0;
}